{{#*inline "page"}}

        <!-- Page Header -->
<header class="masthead" style="background-image: url('http://localhost:8000/img/{{items.0.image_path}}')">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>Tech Blog</h1>
                </div>
            </div>
        </div>
    </div>
</header>

<section id="post">

    <article>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 mx-auto">

                            <h2 class="post-title">
                                {{{ items.0.title }}}
                            </h2>
                    <hr></hr>
                                {{{ items.0.content }}}


                </div>
            </div>
        </div>
    </article>


</section>

{{/inline}}
{{~> (parent)~}}